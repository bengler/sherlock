{
  "from": 0,
  "size": 10,
  "query": {
    "bool": {
      "must": [
        {
          "query_string": {
            "query": "music",
            "default_operator": "AND"
          }
        }
      ]
    }
  },
  "filter": {
    "and": [
      {
        "term": {
          "tags_vector": "rock"
        }
      },
      {
        "and": [
          {
            "term": {
              "restricted": false
            }
          },
          {
            "or": [
              {
                "term": {
                  "published": true
                }
              },
              {
                "missing": {
                  "field": "published",
                  "existence": true,
                  "null_value": true
                }
              }
            ]
          },
          {
            "or": [
              {
                "term": {
                  "deleted": false
                }
              },
              {
                "missing": {
                  "field": "deleted",
                  "existence": true,
                  "null_value": true
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
